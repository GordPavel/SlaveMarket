<xs:schema attributeFormDefault="unqualified" elementFormDefault="qualified"
           xmlns:xs="http://www.w3.org/2001/XMLSchema">
    <xs:element name="database">
        <xs:complexType>
            <xs:sequence>
                <xs:element name="dealList">
                    <xs:complexType>
                        <xs:sequence>
                            <xs:element name="deals">
                                <xs:complexType>
                                    <xs:sequence>
                                        <xs:element name="deal" maxOccurs="unbounded" minOccurs="0">
                                            <xs:complexType>
                                                <xs:sequence>
                                                    <xs:element type="xs:string" name="time"/>
                                                    <xs:element type="xs:string" name="state"/>
                                                    <xs:element name="user">
                                                        <xs:complexType>
                                                            <xs:sequence>
                                                                <xs:element type="xs:string" name="password"/>
                                                                <xs:element type="xs:string" name="token"/>
                                                                <xs:element type="xs:string" name="username"/>
                                                            </xs:sequence>
                                                        </xs:complexType>
                                                    </xs:element>
                                                    <xs:element name="slave" minOccurs="0">
                                                        <xs:complexType>
                                                            <xs:sequence>
                                                                <xs:element type="xs:float" name="height"/>
                                                                <xs:element type="xs:float" name="weight"/>
                                                                <xs:element type="xs:byte" name="age"/>
                                                                <xs:element type="xs:string" name="gender"/>
                                                                <xs:element type="xs:string" name="name"/>
                                                                <xs:element type="xs:short" name="id"/>
                                                            </xs:sequence>
                                                        </xs:complexType>
                                                    </xs:element>
                                                    <xs:element name="alien" minOccurs="0">
                                                        <xs:complexType>
                                                            <xs:sequence>
                                                                <xs:element type="xs:float" name="planet"/>
                                                                <xs:element type="xs:float" name="weight"/>
                                                                <xs:element type="xs:float" name="height"/>
                                                                <xs:element type="xs:float" name="color"/>
                                                                <xs:element type="xs:float" name="name"/>
                                                                <xs:element type="xs:byte" name="age"/>
                                                                <xs:element type="xs:float" name="race"/>
                                                                <xs:element type="xs:short" name="id"/>
                                                            </xs:sequence>
                                                        </xs:complexType>
                                                    </xs:element>
                                                    <xs:element name="food" minOccurs="0">
                                                        <xs:complexType>
                                                            <xs:sequence>
                                                                <xs:element type="xs:float" name="composition"/>
                                                                <xs:element type="xs:float" name="energy"/>
                                                                <xs:element type="xs:float" name="weight"/>
                                                                <xs:element type="xs:short" name="id"/>
                                                                <xs:element type="xs:float" name="name"/>
                                                            </xs:sequence>
                                                        </xs:complexType>
                                                    </xs:element>
                                                    <xs:element name="poison" minOccurs="0">
                                                        <xs:complexType>
                                                            <xs:sequence>
                                                                <xs:element type="xs:float" name="onset"/>
                                                                <xs:element type="xs:short" name="id"/>
                                                                <xs:element type="xs:float" name="frequency"/>
                                                                <xs:element type="xs:float" name="effect"/>
                                                                <xs:element type="xs:float" name="chance"/>
                                                                <xs:element type="xs:float" name="weight"/>
                                                                <xs:element type="xs:float" name="type"/>
                                                                <xs:element type="xs:float" name="name"/>
                                                            </xs:sequence>
                                                        </xs:complexType>
                                                    </xs:element>
                                                    <xs:element type="xs:short" name="price"/>
                                                    <xs:element type="xs:short" name="id"/>
                                                </xs:sequence>
                                            </xs:complexType>
                                        </xs:element>
                                    </xs:sequence>
                                </xs:complexType>
                            </xs:element>
                        </xs:sequence>
                    </xs:complexType>
                </xs:element>
                <xs:element name="users">
                    <xs:complexType>
                        <xs:sequence>
                            <xs:element name="user" maxOccurs="unbounded">
                                <xs:complexType>
                                    <xs:sequence>
                                        <xs:element type="xs:string" name="password"/>
                                        <xs:element type="xs:string" name="token"/>
                                        <xs:element type="xs:string" name="username"/>
                                    </xs:sequence>
                                </xs:complexType>
                            </xs:element>
                        </xs:sequence>
                    </xs:complexType>
                </xs:element>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
</xs:schema>